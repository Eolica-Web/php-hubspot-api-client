{
    "name": "eolica/hubspot-api-client",
    "description": "Hubspot API PHP client",
    "version": "1.0.0",
    "keywords": [
        "eolica",
        "hubspot"
    ],
    "homepage": "https://github.com/Eolica-Web/php-hubspot-api-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Llobell",
            "email": "dllobell@eolicadigital.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "php-http/client-common": "^2.7",
        "php-http/discovery": "^1.20",
        "php-http/message": "^1.16",
        "psr/http-client-implementation": "1.0",
        "psr/http-factory-implementation": "1.0"
    },
    "require-dev": {
        "eolica/coding-standard": "^2.0",
        "guzzlehttp/guzzle": "^7.9",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^4.0",
        "php-http/mock-client": "^1.6",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "rector/rector": "^2.0",
        "symfony/var-dumper": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Eolica\\Hubspot\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Eolica\\Hubspot\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
